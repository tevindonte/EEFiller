{
    "manifest_version" : 3,
    "name" : "EEFiller",
    "description" : "Eric Emmanuel Auto Filler",
    "version" : "1.1",
    "background" : {
        "service_worker" : "welcome/background.js"
    },
    "icons": {
        "16" : "images/16.png",
        "48" : "images/48.png",
        "128" : "images/128.png"
    },
    "page_action" : {
        "default_icon" : {
            "16" : "images/16.png",
            "48" : "images/48.png",
            "128" : "images/128.png"            
        }
    },
    "action" : {
        "default_popup" : "ext/index.html"
    },
    "content_scripts" : [{
        "matches" : ["<all_urls>"],
        "js" : ["content.js"]
        
    }],
    "permissions" : [
        "activeTab",
        "declarativeContent",
        "storage"
        
    ]

}